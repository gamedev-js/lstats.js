{
  "name": "lstats.js",
  "version": "1.2.2",
  "description": "Visualizing and monitor your performance.",
  "main": "dist/lstats.js",
  "scripts": {
    "build:dev": "rollup -c ./script/rollup.config.js",
    "build:min": "uglifyjs ./dist/lstats.dev.js --mangle --source-map ./dist/lstats.min.js.map -o ./dist/lstats.min.js",
    "release": "npm run build:dev && npm run build:min",
    "start": "electron",
    "server": "preview -p 8002 -e examples",
    "dev": "rollup -w -c ./script/rollup.config.js",
    "test": "npm run build:dev && tap test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gamedev-js/lstats.git"
  },
  "keywords": [
    "stats"
  ],
  "author": "jwu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gamedev-js/lstats/issues"
  },
  "homepage": "https://github.com/gamedev-js/lstats/issues",
  "dependencies": {
  },
  "devDependencies": {
    "electron": "^1.6.3",
    "fs-jetpack": "^0.13.0",
    "preview-server": "^1.0.7",
    "rollup": "^0.41.4",
    "rollup-watch": "^3.2.2",
    "uglify-js": "git+https://github.com/mishoo/UglifyJS2.git#harmony"
  }
}
